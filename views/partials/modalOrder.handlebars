<div class="modal" id="modal{{this.id}}">
    <div class="modal-content">
        <div class='container'>
            <table class='highlight'>
                <thead id='order-list' class='list-group'>
                    <tr>
                        <th>Unit of Issue</th>
                        <th>Quantity</th>
                        <th>Add</th>
                    </tr>
                </thead>
                <tbody data-id='{{this.id}}' class='list-group-item'>
                    {{#each order}}
                    <tr>
                        <td>{{this.prodID}}</td>
                        <td>{{this.olUnitofIssue}}</td>
                        <td>{{this.olQuantity}}</td>
                        <td>{{> addOne}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    <div class="modal-footer">
        <a href="#!" class="modal-close waves-effect waves-green btn-flat">Close</a>
    </div>
</div>