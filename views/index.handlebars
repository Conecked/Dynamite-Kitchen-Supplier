<div id='home'>
  <div class='container'>
    <div class='row'>
      <div class='col-8 offset-2'>
        <h1>Order Master</h1>
        <form class='mb-4' action='POST'>
          <div class='form-group flow-text'>
            <label for='products-text'>Products Search</label>
            <input type='text' id='product-text' class='form-control' aria-describedby='product-text' placeholder='Enter Product Text'>
          </div>
          <button id='submit' class='btn btn-primary float-right'>Submit</button>
        </form>
        <h3>Products:</h3>
        <ul id='product-list' class='list-group'>
          {{#each product}}
          <li data-id='{{this.id}}' class='list-group-item'>
            <a href='product/{{this.id}}'>{{this.text}}</a>
            <button class='btn btn-danger float-right delete'>ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
